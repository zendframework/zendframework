{
    "name": "zendframework/zend-debug",
    "description": " ",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "debug"
    ],
    "autoload": {
        "psr-0": {
            "Zend\\Debug\\": ""
        }
    },
    "target-dir": "Zend/Debug",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-escaper": "self.version"
    },
    "suggest": {
        "ext/xdebug": "XDebug, for better backtrace output"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev",
            "dev-develop": "2.2-dev"
        }
    }
}
